#!/usr/bin/env python3
import time


DUDLEY = """
                                        .::-------------::.
                                :++--:                       .:-=+-.
                          .=#:                                        =#:
                         *        .:-=++=:.              .-+++=::.     .  *
                      . -= .-==-.                                  .-===. .*
                      . +++*#+=-:.......                       .....:-=+#*=*.
              ..-*+:       .                                                    -*+:.       .
        . -==.                 :--=====-::..         ...:::-=====---.                 -+=.
      .:+:                :+-:    .-:                          ::.    .-=+.               -+
      %                 +- .  -#:    .+##-.##--%##= +*. *. ..*      *+   . :*                *
     *                 #. .+-               . .   -+=-#=.    .         ==   .#.              .=
     =-                # -=       .==-----==.             ==-----=+.     *:   =              *.
      .-=   .          =-=.      #.   .     :+          +.          #     +. -:            ==
          :*-.  .+=.   :%+*:    .-           *         .#.          *:    + -%+-.  :+=:=*:
              .#. .--   *        *.         =-          =-         :=    ==.:+  .--: .#
               *...  .*.*         .-++===++:.  .          :+*+=+*+:.        .*-=   .  +.
               =:     . *                    .*.  .   +=                    .#.      :#.
                :+.     +:                     :=====-.                     :-    . -+
                  .-=+===*                     :+=-::::                     #::=+==.
                         --                       .                        +.
                          :+                                            ..*  .
                            :+-         =:.                 .=-       :+=..
                           .  . :-#+++-:..  .:::::---:::::::-=+*+=**.    .
                              .#.  -*   -#:**: :+ . .*..#*-+#  .:+   +=
                            .*     .#. .#:..+=**.   :=*=+: :*-..+-     -=
                           +:   :.   :**. :==..:=+=+=: .+=: .%=+.   # :  *:
                          #   :#@ .-=  +     ..=#: :#-.      *.. + .%*:   =-
                        .*     =-   * --             . .     -. *:   *     =-
                        *.   .*     .##:                     :=+      *     =.
                       *:  ..+        %.                     :-       .#    .%.
                      -:    #         #                      :-        -.     +
                     :**====-         #:-=+=====-------=====++:        :*-----#.
                       *.  =:       . %++=---==============--*.        .*    *:
                        *###:         #          -+*.        *         :%**##-
                       . :+:=*.       .      -+==-=--==+=    .        .#::+:.
                             *                    =                   .#
                            %.                .+*#-%=*:           . .  +..
                               :@@@%%#%%#+:..    * +.   ..=#%*++*#@@#=   .
                         .=+:          .-+=.     *.+.   ..=+-.           -+- .
                        *:                 .=*=+ +.*.:=+#                   *
                        +                        ==%.                    .  .*
                       .-+.                      -+#.                     .-*..
                           .:-------==========--:   .---=-===---====----:.


"""


def run_dudley() -> None:
    print(DUDLEY)
    time.sleep(3)
    print("You're stupid Harry")
    time.sleep(1)


if __name__ == "__main__":
    run_dudley()

