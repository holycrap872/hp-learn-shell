"""


def run_hedwig() -> None:
    print(HEDWIG)
    time.sleep(3)
    print("Hoot hoot")
    time.sleep(1)
    print("Hoot hoot")
    time.sleep(3)


if __name__ == "__main__":
    run_hedwig()

